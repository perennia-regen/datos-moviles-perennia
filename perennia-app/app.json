{
  "expo": {
    "name": "Recorrida Holística",
    "slug": "recorrida-holistica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f7f0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.perennia.recorridaholistica",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f5f7f0"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.perennia.recorridaholistica",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Perennia necesita acceso a tu ubicación para geolocalizarte en el mapa de campo."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Perennia necesita acceso a la cámara para tomar fotos de campo."
        }
      ]
    ],
    "scheme": "perennia",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0dea489b-c201-4e7b-bb8a-25975b540c05"
      }
    },
    "owner": "perennia-regen",
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "updates": {
      "url": "https://u.expo.dev/0dea489b-c201-4e7b-bb8a-25975b540c05",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
